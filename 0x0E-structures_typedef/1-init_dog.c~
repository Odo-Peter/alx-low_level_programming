#include "dog.h"
#include <stdlib.h>

/**
 * init_dog - initializes a variable for type struct dog
 * @d: the dog struct
 * @name: the name
 * @age: the age
 * @owner: the owner
 * Return: void on completion
 */

void init_dog(struct dog *d, char *name, float age, char *owner)
{
	d = malloc(sizeof(struct dog));

	d->name = name;
	d->age = age;
	d->owner = owner;
}
